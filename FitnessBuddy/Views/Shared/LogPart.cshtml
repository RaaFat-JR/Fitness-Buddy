@model FitnessBuddy.Models.Login
@using FitnessBuddy.Models

@if (Session["username"] != null)
{
    <ul class="nav navbar-nav navbar-right">
        
        <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
    </ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "Members", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Log in", "Login", "Members", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}